.component {
  width: 100%;
  min-height: 100vh;
}

.bgc-000 {
  width: 100%;
  height: 100%;
  background: #000000;
  opacity: .3;
  position: fixed;
  top: 0;
  left: 0;
}

.bgc-FFF {
  width: 100%;
  height: 600rpx;
  background: #ffffff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  box-sizing: border-box;
}

.dialogTitle {
  background: #F9FAFB;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: center;
}

.fs-32 {
  font-size: 32rpx;
}

.mt-20 {
  margin-top: 20rpx;
  width: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.dialogDay {
  display: inline-block;
  width: 338rpx;
  height: 80rpx;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  margin-right: 20rpx;
  text-align: center;
  line-height: 80rpx;
  font-weight: 700;
}
.bg-FAF6 {
  background: #FAF6ED;
}
.bg-F7F7 {
  background: #F7F7F7;
}
.color-DDDD{
  color: #DDDDDD;
}
.mt-20 .dialogDay:nth-child(2n){
  margin-right: 0rpx;
}
.item-top{
    line-height: 50rpx;
    padding-top: 20rpx;
}
.item-bt{
    line-height: 50rpx;
    padding-bottom: 20rpx;
}
