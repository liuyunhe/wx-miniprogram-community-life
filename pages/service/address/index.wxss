page{
  background: #F9FBFC;
}
.header {
  width: 100%;
  /* height: 140rpx; */
  background: #ffffff;
  box-sizing: border-box;
  padding: 30rpx;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  box-shadow: 0px 1px 4px 1px #DDDDDD;
}
.header image {
  width: 50rpx;
}
.fs-30 {
  font-size: 30rpx;
  margin-bottom: 10rpx;
}
.fs-26 {
  font-size: 24rpx;
  color: #666;
}
.mr-20 {
  margin-right: 20rpx;
}
.ml-10 {
  margin-left: 20rpx;
  /* background: rgb(241, 53, 53); */
  padding: 4rpx 20rpx;
  color: #FF580c;
  border-radius: 20rpx;
  font-size: 24rpx;
  border: 1rpx solid #FF580c;
  border-radius: 8rpx;
}
.foot {
  width: 100%;
  height: 120rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  box-sizing: border-box;
  /* background: #ffffff; */
}
.btn {
  color: #ffffff;
  /* background: #3CDABB; */
  background: -webkit-linear-gradient(left, #1DD394, #46DCCA);
  font-size: 30rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
}
.pb-130 {
  padding: 30rpx;
  padding-bottom: 130rpx;
}