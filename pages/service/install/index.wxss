.header {
  width: 100%;
  height: 100rpx;
  background: #ffffff;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  top: 0;
}
.header image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 40rpx;
  top: 50%;
  transform: translateY(-50%);
}
