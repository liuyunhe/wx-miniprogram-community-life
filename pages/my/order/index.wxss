.goods-list {
  margin: 30rpx;
}
.goods-list .row {
  width: 100%;
  overflow: hidden;
}
.goods-list .row .list_item {
  border-radius: 12rpx;
  position: relative;
  left: -120rpx;
  width: calc(100% + 120rpx);
  height: 243rpx;
}
.goods-list .row .itmem_wrap {
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  position: relative;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
  align-items: center;
  margin-bottom: 30rpx;
  background-color: #fff;
  box-shadow: 0rpx 2rpx 9rpx 0rpx #E8E8E8;
  border-radius: 10rpx;
}
.goods-list .row .itmem_wrap .shop-name {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 43rpx;
  font-size: 28rpx;
  font-weight: 800;
  line-height: 43rpx;
  color: #333333;
  padding-bottom: 7rpx;
  border-bottom: 1rpx solid rgba(0,0,0,0.12);
  margin-bottom: 23rpx;
}
.goods-list .row .itmem_wrap .shop-name image {
  width: 10rpx;
}
.goods-list .row .itmem_wrap .shop-name .status {
  color: #999999;
  font-weight: 500;
}
.goods-list .row .itmem_wrap .check-icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.goods-list .row .itmem_wrap .good-info {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  margin-bottom: 25rpx;
}
.goods-list .row .itmem_wrap .good-info .good-img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.goods-list .row .itmem_wrap .good-info .content {
  width: 400rpx;
  flex: 1;
  height: 160rpx;
}
.goods-list .row .itmem_wrap .good-info .content .title {
  height: 70rpx;
  line-height: 35rpx;
  margin-bottom: 16rpx;
  font-size: 24rpx;
  font-weight: 800;
  color: #333333;
  text-align: justify;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
}
.goods-list .row .itmem_wrap .good-info .content .info {
  height: 40rpx;
  display: flex;
  align-items: center;
}
.goods-list .row .itmem_wrap .good-info .content .type {
  flex: 1;
  display: flex;
}
.goods-list .row .itmem_wrap .good-info .content .type .label {
  padding: 0 15rpx;
  height: 38rpx;
  font-size: 20rpx;
  background: #fafafa;
  line-height: 38rpx;
  text-align: center;
  border-radius: 4rpx;
  opacity: 1;
  border: 1rpx solid #e6e6e6;
  margin-right: 10rpx;
}
.goods-list .row .itmem_wrap .good-info .content .price {
  margin-left: 30rpx;
  height: 38rpx;
  font-size: 20rpx;
  line-height: 38rpx;
  color: #999999;
}
.goods-list .row .itmem_wrap .good-info .content .total-price {
  height: 50rpx;
  font-size: 20rpx;
  line-height: 50rpx;
  color: #666666;
  display: flex;
}
.goods-list .row .itmem_wrap .good-info .content .total-price .footmoney {
  font-size: 24rpx;
  color: #ff580c;
}
.goods-list .row .itmem_wrap .good-info .content .total-price .head {
  font-size: 36rpx;
  color: #ff580c;
  font-weight: bold;
  line-height: 40rpx;
}
.goods-list .row .itmem_wrap .good-info .content .total-price .tail {
  font-size: 20rpx;
  color: #ff580c;
  font-weight: bold;
  line-height: 50rpx;
  position: relative;
  top: 3rpx;
}
.goods-list .row .itmem_wrap .order-line {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  align-items: center;
  padding: 12rpx 0;
}
.goods-list .row .itmem_wrap .order-line .title {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #999999;
  line-height: 30rpx;
  margin-right: 33rpx;
}
.goods-list .row .itmem_wrap .order-line .info {
  flex: 1;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #333333;
  line-height: 30rpx;
}
.info.bottom {
  width: 100%;
}
.btn-groups {
  display: flex;
  justify-content: flex-end;
  flex: 1;
}
.btn-groups .btn {
  width: 120rpx;
  height: 44rpx;
  border-radius: 4rpx;
  border: 2rpx solid rgba(153, 153, 153, 0.5);
  line-height: 44rpx;
  text-align: center;
  color: #000;
  font-size: 20rpx;
  margin-left: 10rpx;
  margin-top: 0;
  padding: 0;
}
.btn-groups .btn.main {
  border: 2rpx solid #3CDABB;
  color: #3CDABB;
}