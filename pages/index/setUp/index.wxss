page{
  background-color: #F9FBFC;
  /* padding: 30rpx; */
}
.bg-FFFF {
  background: #fff;
}
.view-column{
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  background: #fff;
  border-radius: 10rpx;
  margin: 30rpx;
  padding: 30rpx;
}
.view-row{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  color: #333;
}
.view-shu{
  width: 6rpx;
  height: 35rpx;
  background: #3CDABB;
}
.view-line {
  width: 100%;
  height: 1rpx;
  background: #f7f7f7;
  /* margin-top: 20rpx;
  margin-left: 30rpx; */
}
.text-btn{
  color: #999;
  background: #fff;
  border-radius: 10rpx;
  border: 1rpx solid #ccc;
  padding: 5rpx 20rpx;
  margin-left: 30rpx;
}
.header {
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  font-weight: 700;
  color: #000000;
  border-bottom: 1rpx solid #f7f7f7;
  margin-bottom: 30rpx;
}
.bg-FFFF image {
  width: 80rpx;
  margin-right: 10rpx;
}
.inp {
  border: 1rpx solid #cccccc;
  width: 100%;
  height: 80rpx;
  border-radius: 10rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
}
.foot {
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #333;
  padding-bottom: 20rpx;
}
.mb-20{
  margin-bottom: 20rpx;
}
.flex-1 {
  flex: 1;
}
.mt-20 {
  margin-top: 20rpx;
}
.btn {
  width: 100%;
  height: 80rpx;
  color: #ffffff;
  background: #26a886;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
}
.view-foot {
  background: #fff;
  /* padding: 30rpx; */
  margin-top: 20rpx;
  font-size: 30rpx;
}
.view-line {
  width: 100%;
  height: 1px;
  background: #f8f8f8;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.active-color{
  color: #3CDABB;
  /* background: #3CDABB; */
  background: #D8F8F1;
  border-radius: 10rpx;
  border: 1rpx solid #3CDABB;
  padding: 5rpx 20rpx;
  margin-left: 30rpx;
}
