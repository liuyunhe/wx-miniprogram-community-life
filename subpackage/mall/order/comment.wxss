.view-back {
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 9rpx 0rpx rgba(40, 17, 78, 0.04);
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.view-title {
  font-size: 32rpx;
}
.view-star {
  height: 90rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 9rpx 0rpx rgba(40, 17, 78, 0.04);
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  padding: 0 30rpx;
}
.view-star .title {
  font-size: 24rpx;
  font-weight: 500;
  color: #333333;
  margin-right: 30rpx;
}
.view-star .star-wrap .star {
  display: block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.view-star .star-type {
  color: #999999;
  font-weight: 500;
  font-size: 24rpx;
  line-height: 28rpx;
  margin-left: 30rpx;
}
.btn-enter {
  background: #26a886;
  color: #fff;
  font-size: 30rpx;
  margin-top: 50rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
textarea {
  /* border: 3rpx solid #DDD; */
  width: 100%;
  font-size: 30rpx;
}
.view-ly {
  display: flex;
  flex-direction: row;
  width: 620rpx;
  margin-bottom: 32rpx;
  align-items: center;
}
.text-ly {
  flex: 1;
}
.tab-bar {
  display: flex;
  flex-flow: row;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 30rpx;
  justify-content: space-between;
}
.view-flex {
  display: flex;
  align-items: center;
  justify-content: center;
  /* margin-right: 30rpx; */
  margin-bottom: 20rpx;
}
.text-tab {
  text-align: center;
  font-size: 28rpx;
  flex: 1;
  /* border-width: 0.5px;
    border-color:#DDDDDD;
    border-radius: 35rpx; */
  /* border-style: solid; */
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 12rpx;
  padding-bottom: 12rpx;
  width: 120rpx;
  background: #E6E6E6;
}
.view-column {
  display: flex;
  flex-flow: column;
  /* align-items: center; */
  padding: 20rpx 30rpx;
  font-weight: bold;
  height: 100vh;
  background-color: #F9FBFC;
}
.input-tel {
  background: #E6E6E6;
  font-size: 26rpx;
  padding: 12rpx;
  margin-top: 30rpx;
}
.text-color {
  background: #26a886;
  color: #fff;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
  font-size: 28rpx;
}
.upload-img-title {
  color: #333;
  font-size: 24rpx;
}
