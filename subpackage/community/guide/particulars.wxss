/* pages/community/guide/particulars.wxss */
.page{
  background: white;
}
.title{
  text-align: center;
  /* font-weight: bold; */
  font-size: medium;
}
/*分割线样式*/
.divLine{
  background: #E0E3DA;
  margin-top: 4px;
  width: 100%;
  height: 5rpx;
}
.root{
  background-color: #F9FBFC;
  min-height: 100vh;
  padding-bottom: 100rpx;
  box-sizing: border-box;
}
.root::before{
  content: "";
  display: table;
}
.rootview{
  position: relative;
  background-color: #FFFFFF;
  margin: 10pt 10pt 20pt 10pt;
  padding: 30rpx;
  box-shadow: 0rpx 4rpx 7rpx 0rpx rgba(0,0,0,0.09);
  border-radius: 10rpx;
}
.detailStyle{
  margin-top: 10pt;
  font-size: 30rpx;
  line-height: 42rpx;
}