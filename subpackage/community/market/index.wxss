.view-switch {
  width: 480rpx;
  height: 70rpx;
  margin: 30rpx auto;
  border-radius: 6rpx;
  border: 2rpx solid #3CDABB;
}

.view-switch .item{
  float: left;
  width: 50%;
  line-height: 70rpx;
  text-align: center;
  font-size: 36rpx;
  color: #3CDABB;
}
.view-switch .item.active{
  background: linear-gradient(147deg, #1DD394 0%, #46DCCA 100%);
  color: #fff;
}

.view-order-item {
  display: flex;
  flex-flow: row;
  height: 220rpx;
}

.img-class {
  display: flex;
  position: relative;
  align-items: center;
  /* height: 220rpx; */
  margin-left: 10rpx;
  margin-top: 16rpx;
}
/* 图片的阴影遮罩，使用时应将需要遮罩的图片的父容器设置为相对定位 */
.maskImage{
  background: rgba(70, 70, 70, 0.63);
  position: absolute;
  color:#FFFFFF;
  font-size: 26rpx;
  text-align: center;
  line-height: 200rpx;
}
.alternative{
  background: rgba(70, 70, 70, 0.63);
}
.text-price {
  display: flex;
  flex-flow: column;
  text-align: left;
  margin-left: 20rpx;
}

.price-title {
  font-size: 30rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 33rpx;
  margin-top: 24rpx;
  width: 300rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  /* white-space: nowrap; */
}

.price-hint {
  font-size: 28rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  width: 270rpx;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow-x: hidden;
  color: #999999;
  height: 34rpx;
  line-height: 28rpx;
  margin-top: 10rpx;
}

.price-bottom {
  display: flex;
  flex-flow: row;
