/* pages/community/question/index.wxss */
.header {
  width: 100%;
  height: 120rpx;
  background: #ffffff;
  font-size: 30rpx;
  position: relative;
  box-sizing: border-box;
}
.title{
  font-weight:Medium;
  font-family: PingFang;
  color: #333333;
  padding-left: 30rpx;
  padding-bottom: 9rpx;
}
.detailLimits{
  /* overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis; */
  color: #666666;
  font-size: 26rpx;
  font-family:PingFang;
  width:600rpx;
  margin-left:30rpx;
}
.img {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 70rpx;
  top: 50%;
  transform: translateY(-50%);
}

.serchbtn {
  margin-left: 20rpx;
}

.input {
  width: 580rpx;
  height: 60rpx;
  border: 1rpx solid #cccccc;
  border-radius: 30rpx;
  background: #f7f7f7;
  padding-left: 50rpx;
  box-sizing: border-box;
}
.content {
  margin-top: 20rpx;
}

.order-row .state{
  border: 1px solid #FF773A;
  border-radius: 4rpx;
  line-height: 32rpx;
  font-size: 22rpx;
  /* width: 400rpx; */
  padding: 0 10rpx;
  margin-left: 20rpx;
  color: #FF773A;
}

.order-row .state.solve{
  border: 1px solid #3CDABB;
  color: #3CDABB;
}

.order-row .state.complete{
  border: 1px solid #B8B8B8;
  color: #B8B8B8;
}
.order-row .phone{
  flex: 1;
  text-align: right;
}

